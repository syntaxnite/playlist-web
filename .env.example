# Spotify API Configuration
VITE_SPOTIFY_CLIENT_ID=your_spotify_client_id_here

# Apple Music API Configuration  
VITE_APPLE_CLIENT_ID=your_apple_client_id_here

# YouTube Music API Configuration
VITE_YOUTUBE_CLIENT_ID=your_youtube_client_id_here

# Deezer API Configuration
VITE_DEEZER_CLIENT_ID=your_deezer_client_id_here

# Tidal API Configuration
VITE_TIDAL_CLIENT_ID=your_tidal_client_id_here

# API Base URL (for auth callbacks)
VITE_API_BASE_URL=http://localhost:3000

# OAuth Callback URLs (use these when registering your apps)
# Spotify: http://localhost:3000/auth/spotify/callback
# Apple Music: http://localhost:3000/auth/apple/callback  
# YouTube Music: http://localhost:3000/auth/youtube/callback
# Deezer: http://localhost:3000/auth/deezer/callback
# Tidal: http://localhost:3000/auth/tidal/callback